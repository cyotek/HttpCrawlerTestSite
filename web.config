<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.php" />
            </files>
        </defaultDocument>

        <staticContent>
            <remove fileExtension=".apng" />
            <remove fileExtension=".webp" />
            <remove fileExtension=".tiff" />
            <remove fileExtension=".svg" />
            <mimeMap fileExtension=".apng" mimeType="image/apng" />
            <mimeMap fileExtension=".webp" mimeType="image/webp" />
            <mimeMap fileExtension=".tiff" mimeType="image/tiff" />
            <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
            <mimeMap fileExtension=".avif" mimeType="image/avif" />
            <mimeMap fileExtension=".jxl" mimeType="image/jxl" />
        </staticContent>
    </system.webServer>
</configuration>
